    "def solution(n):\n",
    "    chargen = 0\n",
    "    \n",
    "    while n >0:\n",
    "        if n % 2 ==1:\n",
    "            n -= 1\n",
    "            chargen += 1\n",
    "        elif n % 2 ==0:\n",
    "            n /= 2\n",
    "    \n",
    "\n",
    "\n",
    "    return chargen"
   ]
