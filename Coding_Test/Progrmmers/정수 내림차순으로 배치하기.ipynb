    "def solution(n):\n",
    "    x = []\n",
    "    \n",
    "    for i in str(n):\n",
    "        x.append(i)\n",
    "    \n",
    "    x.sort(reverse = True)\n",
    "    \n",
    "    answer = ''\n",
    "    for i in x:\n",
    "        answer = answer + i\n",
    "        \n",
    "    answer = int(answer)\n",
    "    return answer"
