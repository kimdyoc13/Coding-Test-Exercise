    "def solution(x):\n",
    "    x = str(x)\n",
    "    x_list = []\n",
    "    \n",
    "    for i in x:\n",
    "        x_list.append(i)\n",
    "    \n",
    "    t = sum(x_list)\n",
    "    \n",
    "    if x % t >=0:\n",
    "        answer = False\n",
    "    elif x % t<0:\n",
    "        answer = True\n",
    "    return answer"
   ]
