    "def solution(arr):\n",
    "    check = 10\n",
    "    answer = []\n",
    "    \n",
    "    for i in arr:\n",
    "        if i == check:\n",
    "            continue\n",
    "        answer.append(i)\n",
    "        check = i\n",
    "\n",
    "    return answer"
   ]
