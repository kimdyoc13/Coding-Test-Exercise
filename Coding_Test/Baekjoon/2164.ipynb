    "from collections import deque\n",
    "\n",
    "def find_last_card(N):\n",
    "    queue = deque(range(1, N+1))\n",
    "\n",
    "    while len(queue) > 1:\n",
    "        queue.popleft() \n",
    "        queue.append(queue.popleft())\n",
    "    return queue[0]\n",
    "\n",
    "N = int(input())\n",
    "print(find_last_card(N))"
   ]
