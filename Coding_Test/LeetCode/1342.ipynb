    "class Solution:\n",
    "    def numberOfSteps(self, num: int) -> int:\n",
    "        x = 0\n",
    "        while num != 0:\n",
    "            if num % 2  == 0:\n",
    "                num = num / 2\n",
    "                x = x+1\n",
    "            elif num % 2 ==1:\n",
    "                num = num-1\n",
    "                x = x+1\n",
    "        return x"
   ]
